/*
    expansive.json - Configuration for esp-html-skeleton
 */
{
    dev: {
        services: {
            "?gzip": false,
            "?html": false,
            "?js": {
                "?minify": false,
                "?usemin": true,
                "?usemap": true
            }
        }
    },
    prod: {
        services: {
            "css": {
                "?minify": true,
                "?usemin": true,
                "?usemap": false
            },
            "?gzip": true,
            "js": {
                "?minify": true,
                "?usemin": true,
                "?usemap": false
            }
        }
    }
}
