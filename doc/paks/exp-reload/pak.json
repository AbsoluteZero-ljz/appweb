{
    "name": "exp-reload",
    "description": "Live browser reload plugin for Expansive",
    "version": "1.0.0",
    "keywords": [
        "expansive-plugin",
        "reload"
    ],
    "licenses": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-reload.git"
    },
    "dependencies": {
        "jquery": "~3"
    },
    "devDependencies": {
        "expansive": ">=1.0.0",
        "pak": ">=1.0.0"
    },
    "export": [
        {
            "from": [
                "partials/**",
                "contents/**"
            ],
            "to": "${CONTENTS}/.."
        },
        {
            "from": "*.js.exp",
            "to": "exp-reload"
        }
    ],
    "origin": "embedthis/exp-reload"
}
