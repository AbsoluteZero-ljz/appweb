{
    "name": "exp-markdown",
    "description": "Markdown Plugin for Expansive",
    "version": "0.3.0",
    "keywords": [
        "expansive-plugin",
        "marked"
    ],
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-markdown.git"
    },
    "engines": {
        "node": ">= 0.4.0"
    },
    "devDependencies": {
        "expansive": ">=0.7.0",
        "pak": ">=0.11.3"
    },
    "ignore": [
        "**/doc/**",
        "**/man/**"
    ],
    "scripts": {
        "postcache": {
            "script": "npm('install -g', 'marked')"
        }
    },
    "origin": "embedthis/exp-markdown"
}
